   {
      "id": "b2a165a7.1c76e8",
      "type": "function",
      "z": "163c4993.e4c056",
      "name": "Echo Value",
      "func": "\n\nvar deviceID = msg.payload.deviceID;\n\n\n\nDate.prototype.yyyymmdd = function() {\n  var mm = this.getMonth() + 1; // getMonth() is zero-based\n  var dd = this.getDate();\n\n  return [this.getFullYear(), !mm[1] && '0', mm, !dd[1] && '0', dd].join(''); // padding\n};\n\n\nvar date = new Date();\nvar RegisteredDate = date.yyyymmdd();\nvar dateStamp = RegisteredDate;\n\nvar time = new Date();\nvar RegisteredTime = time.getTime();\nvar timeStamp = RegisteredTime;\n\nvar tableChanger = \"powerTable\";\n\nif(deviceID==1){\n\ttableChanger = \"powerTable\";\n\n}else if(deviceID==2){\n\n\ttableChanger= \"powerTable2\";\n\n}else if(deviceID==3){\n\n\ttableChanger =\"powerTable3\";\n\n}\n\n\nDate.prototype.yyyymmdd = function() {\n  var mm = this.getMonth() + 1; \n  var dd = this.getDate();\n\n  return [this.getFullYear(), !mm[1] && '0', mm, !dd[1] && '0', dd].join('');\n};\n\nvar date = new Date();\nvar RegisteredDate = date.yyyymmdd();\nvar dateStamp = RegisteredDate;\n\nvar time = new Date();\nvar RegisteredTime = time.getTime();\nvar timeStamp = RegisteredTime;\n\n\n\nvar powerString =msg.newload.powerValue;\nvar powerValue = Number(powerString);\n\nvar dateChecker =0;\nvar jsonText = msg.payload;\nvar jsonCount = jsonText.length;\n\nfor(i=0; i<jsonCount; i++){\n\n\tif(jsonText[i].dateStamp== dateStamp ){\n\t\tdateChecker = 1;\n\t\tvar powerValueJson = jsonText[i].powerValue; \n\t\tvar powerConsumedBefore = jsonText[i].powerConsumed;\n\t}\n\t\n\n}\n\nif(dateChecker ==1){\n\n\tvar jsonReady = String(powerValueJson).trim(); \n    var powerValueList = jsonReady.replace(/\\0[\\s\\S]*$/g,'');\n\tvar jsonObject = JSON.parse(powerValueList);\n\tvar powerValueCount = Object.keys(jsonObject).length;\n\n\t\n\t\n\tvar reTable = new Array();\n\n\tfor(i=0; i<powerValueCount; i ++){\n\n\t\tvar mover = new Object();\n\t\tmover['timeStamp'] = jsonObject[i].timeStamp;\n\t\tmover['powerValue'] = jsonObject[i].powerValue;\n\t\tmover['powerConsumed'] = jsonObject[i].powerConsumed;\n\t\treTable[i] = mover;\n\n\t}\n        \n\t    var newEntry = new Object();\n\t\tnewEntry['timeStamp'] = timeStamp;\n\t\tnewEntry['powerValue'] = powerValue;\n\n\t\tvar previousTimeStamp = jsonObject[powerValueCount -1].timeStamp;\n\t\tvar previousPowerValueString = jsonObject[powerValueCount -1].powerValue;\n\t\tvar previousPowerValue = Number(previousPowerValueString);\n\n\t\t\n\t\tvar calcA = (timeStamp - previousTimeStamp)/1000;\n\t\tvar calcB = powerValue + previousPowerValue;\n\n\t\tvar calcT = (calcA*calcB)/7200 ;\n        newEntry['powerConsumed'] = calcT;\n       \n\n        var powerConsumedBeforeNumber = Number(powerConsumedBefore);\n\n\t\t\n\t\tvar powerConsumedReady = calcT + powerConsumedBeforeNumber;\n\t\tvar powerConsumed = powerConsumedReady.toFixed(2);\n\n\t\t\n\t\treTable[powerValueCount] = newEntry;\n\n\t\tvar jsonReady = JSON.stringify(reTable);\n        var newValueJson =\tjsonReady.replace(/\\0[\\s\\S]*$/g,'');\n\n\t\tvar msg;\n\n\t\tmsg = {\n\t\t\ttopic :\"UPDATE \" + tableChanger + \" SET powerValue= '\" + newValueJson +\"', powerConsumed= \" + powerConsumed + \" WHERE dateStamp= \" + dateStamp\n\t\t}\n\n\t\treturn msg;\n\n\n\n\n\n\n\n}else if(dateChecker == 0){\n\n\t\t\n\tvar reTable = new Array();\n\tvar mover = new Object();\n\tmover['timeStamp']= timeStamp;\n\tmover['powerValue'] = powerValue;\n\tmover['powerConsumed'] = 0;\n\treTable[0] = mover;\n\n\tvar jsonReady = JSON.stringify(reTable);\n    var newValueJson =\tjsonReady.replace(/\\0[\\s\\S]*$/g,'');\n\tvar msg;\n\n\tmsg ={\n\t\ttopic:\"INSERT INTO \" + tableChanger + \" SET dateStamp=\" + dateStamp +\", powerValue= '\" + newValueJson +\"', powerConsumed=0\"\n\t}\n\n\treturn msg;\n\n\n}\n",
